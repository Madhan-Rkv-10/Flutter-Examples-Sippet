    "            children: <Widget>[",
    "              BorderedBox(",
    "                shape: StadiumBorder(",
    "                  side: BorderSide(",
    "                    color: borderColor,",
    "                    width: borderWidth,",
    "                    strokeAlign: (animation.value * 2) - 1,",
    "                  ),",
    "                ),",
    "              ),",
    "              Row(",
    "                mainAxisAlignment: MainAxisAlignment.spaceAround,",
    "                children: <Widget>[",
    "                  BorderedBox(",
    "                    shape: CircleBorder(",
    "                      side: BorderSide(",
