    "    return Scaffold(",
    "      appBar: AppBar(title: Text(\"SizeTransition\")),",
    "      body: SizeTransition(",
    "        sizeFactor: _animation,",
    "        axis: Axis.horizontal,",
    "        axisAlignment: -1,",
    "        child: const Center(",
    "          child: FlutterLogo(size: 200.0),",
    "        ),",
