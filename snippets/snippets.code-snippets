      "            const SizedBox(height: 10.0),",
      "            Wrap(",
      "              spacing: 5.0,",
      "              children: List<Widget>.generate(",
      "                3,",
      "                (int index) {",
      "                  return ChoiceChip(",
      "                    label: Text('Item $index'),",
      "                    selected: _value == index,",
      "                    onSelected: (bool selected) {",
      "                      setState(() {",
      "                        _value = selected ? index : null;",
      "                      });",
      "                    },",
      "                  );",
      "                },",
      "              ).toList(),",
      "            ),",
      "          ],",
      "        ),",
      "      ),",
      "    );",
      "  }",
      "}"
