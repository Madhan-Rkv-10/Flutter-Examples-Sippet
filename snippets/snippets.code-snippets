  "Flutter CheckboxListTile Example": {
    "scope": "dart",
    "prefix": "flutter-CheckboxListTile",
    "body": [
      "import 'package:flutter/material.dart';",
      "",
      "void main() => runApp(const CheckboxListTileApp());",
