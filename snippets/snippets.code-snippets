  "prefix": "flutter-Appbar-custom",
  "body": [
    "",
    "import 'package:flutter/material.dart';",
    "",
    "/// Flutter code sample for [PreferredSize].",
    "",
    "void main() => runApp(const PreferredSizeExampleApp());",
    "",
