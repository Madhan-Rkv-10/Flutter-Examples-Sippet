    "            ),",
    "            const PopupMenuItem<SampleItem>(",
    "              value: SampleItem.itemThree,",
    "              child: Text('Item 3'),",
    "            ),",
    "          ],",
    "        ),",
    "      ),",
    "    );",
    "  }",
