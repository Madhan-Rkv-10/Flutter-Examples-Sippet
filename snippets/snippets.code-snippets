    "/// Flutter code sample for [Tooltip].",
    "",
    "void main() => runApp(const TooltipExampleApp());",
    "",
    "class TooltipExampleApp extends StatelessWidget {",
    "  const TooltipExampleApp({super.key});",
    "",
    "  @override",
