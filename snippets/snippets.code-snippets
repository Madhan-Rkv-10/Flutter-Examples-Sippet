    "  @override",
    "  Widget build(BuildContext context) {",
    "    return MaterialApp(",
    "      home: Scaffold(",
    "        appBar: AppBar(title: const Text('SliverAnimatedOpacity Sample')),",
    "        body: const Center(",
    "          child: SliverAnimatedOpacityExample(),",
    "        ),",
    "      ),",
